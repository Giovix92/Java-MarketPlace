                            <font>
                                <Font name="Roboto" size="13.0"/>
                            </font>
                            <VBox.margin>
                                <Insets top="5.0"/>
                            </VBox.margin>
                        </TextField>
                        <Label fx:id="sellerLabel" text="Venditore">
                            <font>
                                <Font name="Roboto" size="16.0"/>
