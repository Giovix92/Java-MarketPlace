                                <Insets left="20.0" right="20.0" top="5.0"/>
                            </VBox.margin>
                            <font>
                                <Font name="Roboto" size="13.0"/>
                            </font>
                        </TextField>
                        <Label fx:id="descriptionLabel" text="Descrizione prodotto">
                            <font>
                                <Font name="Roboto" size="16.0"/>
