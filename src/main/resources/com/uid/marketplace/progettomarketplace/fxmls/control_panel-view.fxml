                                <TextField fx:id="ProductPriceBar" prefHeight="25.0" prefWidth="110.0">
                                    <HBox.margin>
                                        <Insets left="30.0" top="5.0"/>
                                    </HBox.margin>
                                </TextField>
                                <MenuButton fx:id="CategoriesProductButton" mnemonicParsing="false" prefHeight="25.0"
